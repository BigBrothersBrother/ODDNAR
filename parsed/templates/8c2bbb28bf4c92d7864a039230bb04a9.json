{"components/common/subscription-form":"<strong class=\"footer-info-heading\">$25 Off Your First Order</strong>\r\n<p>{{lang 'newsletter.subscribe_intro'}}</p>\r\n\r\n<form class=\"form\"  name=\"campaignerForm\" action=\"https://secure.campaigner.com/CSB/Public/ProcessHostedForm.aspx\" method=\"post\"  enctype=\"multipart/form-data\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <input type=\"hidden\" name=\"__VIEWSTATEGENERATOR\" id=\"__VIEWSTATEGENERATOR\" value=\"5523BD75\">\r\n        <input type=\"hidden\" name=\"FormInfo\" id=\"FormInfo\" value=\"bebfe99f-1b9c-4382-a479-9b4a9b4dbd26\">\r\n        <div class=\"form-field\">\r\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\r\n            <div class=\"form-prefixPostfix wrap\">\r\n                <input class=\"form-input\" id=\"nl_email\" contactattributeid=\"1461320\" name=\"1461320\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\r\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\r\n            </div>\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n\r\n\r\n"}
