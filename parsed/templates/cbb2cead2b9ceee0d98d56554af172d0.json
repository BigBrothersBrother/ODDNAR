{"components/common/share":"{{#if settings.add_this.buttons}}\r\n    <div class=\"addthis_toolbox addthis_32x32_style\" addthis:url=\"{{url}}\"\r\n         addthis:title=\"{{title}}\">\r\n         <span>Share This Item</span>\r\n        <ul class=\"socialLinks\">\r\n\r\n          <li class=\"socialLinks-item socialLinks-item--facebook\">\r\n            <a class=\"addthis_button_facebook icon icon--facebook\" title=\"Facebook\" href=\"#\">\r\n              <svg>\r\n                  <use xlink:href=\"#icon-facebook\"></use>\r\n              </svg>\r\n            </a>\r\n          </li>\r\n          <li class=\"socialLinks-item socialLinks-item--twitter\">\r\n            <a class=\"addthis_button_twitter icon icon--twitter\" title=\"Twitter\" href=\"#\">\r\n              <svg>\r\n                  <use xlink:href=\"#icon-twitter\"></use>\r\n              </svg>\r\n            </a>\r\n          </li>\r\n          <li class=\"socialLinks-item socialLinks-item--email\">\r\n            <a class=\"addthis_button_email icon icon--email\" target=\"_blank\" title=\"Email\" href=\"#\">\r\n              <svg>\r\n                <use xlink:href=\"#icon-envelope\"></use>\r\n              </svg>\r\n            </a>\r\n          </li>\r\n          <li class=\"socialLinks-item socialLinks-item--print\">\r\n            <a class=\"addthis_button_print icon icon--print\" title=\"Print\" href=\"#\">\r\n              <svg>\r\n                  <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-print\"></use>\r\n              </svg>\r\n            </a>\r\n           </li>\r\n        </ul>\r\n        <script type=\"text/javascript\"\r\n                src=\"//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-4e94ed470ee51e32\"></script>\r\n        <script>\r\n            if (typeof(addthis) === \"object\") {\r\n                addthis.toolbox('.addthis_toolbox');\r\n            }\r\n        </script>\r\n    </div>\r\n{{/if}}\r\n"}
