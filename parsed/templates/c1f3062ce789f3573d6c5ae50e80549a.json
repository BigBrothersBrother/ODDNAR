tribute=\"input-text\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\r\n        {{ this.display_name }}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    <input class=\"form-input form-input--small {{ this.display_name }}\" type=\"text\" id=\"attribute_{{id}}\" name=\"attribute[{{this.id}}]\" class=\"{{ this.display_name }}\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\r\n</div>\r\n","components/products/options/product-list":"<div class=\"form-field {{this.display_name}}\" data-product-attribute=\"product-list\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        {{display_name}}:\r\n        <span data-option-value></span>\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    {{#if values.0.image}}\r\n        <ul class=\"productOptions-list\">\r\n            {{#unless required}}\r\n                <li class=\"productOptions-list-item\">\r\n                    <input class=\"form-radio\"\r\n                           type=\"radio\"\r\n                           name=\"attribute[{{id}}]\"\r\n                           value=\"0\"\r\n                           id=\"attribute_0_{{id}}\"\r\n                           checked=\"{{#if defaultValue '==' 0}}checked{{/if}}\" required>\r\n                    <label class=\"form-label\" for=\"attribute_0_{{id}}\">{{lang 'products.none'}}</label>\r\n                </li>\r\n            {{/unless}}\r\n            {{#each values}}\r\n                <li class=\"productOptions-list-item\" data-product-attribute-value=\"{{id}}\">\r\n                    {{#if image}}\r\n                        <figure class=\"productOptions-list-item-figure\">\r\n                            <img class=\"productOptions-list-item-image\" src=\"{{getImage image 'productthumb_size'}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\r\n                        </figure>\r\n                    {{/if}}\r\n                    <div class=\"productOptions-list-item-content\">\r\n                        <input\r\n                            class=\"form-radio\"\r\n                            type=\"radio\"\r\n                            name=\"attribute[{{../id}}]\"\r\n                            value=\"{{id}}\"\r\n                            id=\"attribute_{{id}}\"\r\n                            {{#if selected}}checked{{/if}}\r\n                            {{#if ../required}}required{{/if}}>\r\n                        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_{{id}}\">{{label}}</label>\r\n                    </div>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n    {{else}}\r\n        {{#unless required}}\r\n        <input class=\"form-radio\"\r\n               type=\"radio\"\r\n               name=\"attribute[{{id}}]\"\r\n               value=\"0\"\r\n               id=\"attribute_0_{{id}}\"\r\n               {{#if defaultValue '==' 0}}checked{{/if}}>\r\n        <label class=\"form-label\" for=\"attribute_0_{{id}}\">{{lang 'products.none'}}</label>\r\n        {{/unless}}\r\n        {{#each values}}\r\n            <input\r\n                class=\"form-radio\"\r\n                type=\"radio\"\r\n                name=\"attribute[{{../id}}]\"\r\n                value=\"{{id}}\"\r\n                id=\"attribute_{{id}}\"\r\n                {{#if selected}}checked{{/if}}\r\n                {{#if ../required}}required{{/if}}>\r\n            <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_{{id}}\">{{label}}</label>\r\n        {{/each}}\r\n    {{/if}}\r\n</div>\r\n","components/products/options/set-radio":"<div class=\"form-field {{this.display_name}}\" data-product-attribute=\"set-radio\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        {{ this.display_name }}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    {{#each this.values}}\r\n        <input\r\n            class=\"form-radio\"\r\n            type=\"radio\"\r\n            id=\"attribute_{{id}}\"\r\n            name=\"attribute[{{../id}}]\"\r\n            value=\"{{id}}\"\r\n            data-text=\"{{this.label}}\"\r\n            {{#if selected}}checked{{/if}}\r\n            {{#if ../required}}required{{/if}}>\r\n        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_{{id}}\">{{this.label}}</label>\r\n    {{/each}}\r\n</div>\r\n","components/products/options/set-rectangle":"<div class=\"form-field {{this.display_name}}\" data-product-attribute=\"set-select\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        <span class=\"mobhide\">SELECT SIZE</span>\r\n    </label>\r\n\r\n    <select class=\"form-select form-select--small form-select-size\" name=\"attribute[{{this.id}}]\" id=\"attribute_select_{{id}}\" {{#if required}}required{{/if}}>\r\n    <option value=\"\">Select Size</option>\r\n        {{#each this.values}}\r\n            <option data-product-attribute-value=\"{{id}}\" value=\"{{id}}\">\r\n                {{label}}\r\n            </option>\r\n        {{/each}}\r\n    </select>\r\n</div>\r\n","components/products/options/set-select":"<div class=\"form-field {{this.display_name}}\" data-product-attribute=\"set-select\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\r\n         <span class=\"mobhide\">PLEASE SELECT</span> {{ this.display_name }}\r\n    </label>\r\n\r\n    <select class=\"form-select form-select--small\" name=\"attribute[{{this.id}}]\" id=\"attribute_select_{{id}}\" {{#if required}}required{{/if}}>       \r\n        {{#each this.values}}\r\n            <option data-product-attribute-value=\"{{id}}\" value=\"{{id}}\" {{#if selected}}selected{{/if}}> {{#if label '===' 'Serged Cotton'}} Serged Cotton + $8.00 {{else}}{{label}}{{/if}}</option>\r\n        {{/each}}\r\n    </select>\r\n</div>\r\n","components/products/options/swatch":"<div class=\"form-field {{this.display_name}}\" data-product-attribute=\"swatch\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        <span class=\"mobhide\">PLEASE SELECT</span> {{this.display_name}}\r\n        <span data-option-value></span>\r\n    </label>\r\n    <div class=\"swipeleft\">Swipe Left For More</div>\r\n    <div id=\"optionscroll\">\r\n    {{#each this.values}}\r\n        <input class=\"form-radio {{this.label}} swatchinput\" type=\"radio\" name=\"attribute[{{../id}}]\" value=\"{{id}}\" id=\"attribute_{{id}}\" {{#if selected}}checked{{/if}} {{#if ../required}}required{{/if}}>\r\n        <label class=\"form-option form-option-swatch swatchselect {{this.label}}\" for=\"attribute_{{id}}\" data-product-attribute-value=\"{{id}}\" data-swatch=\"{{this.label}}\">\r\n            {{#if image}}\r\n                <span class='form-option-variant form-option-variant--pattern' title=\"{{this.label}}\" style=\"background-image: url('{{getImage image \"small\"}}');\"></span>\r\n            {{/if}}\r\n            {{#if data.[2]}}\r\n                <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\r\n                <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\r\n                <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[2]}}\"></span>\r\n            {{else}}\r\n                {{#if data.[1]}}\r\n                    <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\r\n                    <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\r\n                {{else}}\r\n                    {{#if data.[0]}}\r\n                        <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\r\n                    {{/if}}\r\n                {{/if}}\r\n            {{/if}}\r\n            {{#if pattern}}\r\n                <span class=\"form-option-expanded\">\r\n                    <span class=\"form-option-image\" style=\"background-image: url('{{getImage image 'core-swatch'}}');\"></span>\r\n                    <span class=\"expanded-title\">{{this.label}}</span>\r\n                </span>\r\n            {{/if}}\r\n        </label>\r\n    {{/each}}\r\n</div>\r\n</div>\r\n","components/products/options/textarea":"<div class=\"form-field {{this.display_name}}\" data-product-attribute=\"textarea\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\r\n        {{display_name}}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    <textarea class=\"form-input\" id=\"attribute_{{id}}\" name=\"attribute[{{id}}]\" {{#if required}}required{{/if}}>\r\n        {{prefill}}\r\n    </textarea>\r\n</div>\r\n","components/products/price":"{{#or has_options product.options.length}}\r\n    {{#if price.with_tax}}\r\n        <div class=\"price-section price-section--withTax\" {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_with_tax}}\r\n                <span data-product-rrp-with-tax class=\"price price--rrp\">{{price.rrp_with_tax.formatted}}</span>\r\n            {{/if}}\r\n                {{#if theme_settings.price_as_low_as}}\r\n                    <span class=\"lowas\" translate>Starting at </span>\r\n                {{/if}}\r\n            <span data-product-price-with-tax class=\"price\">{{price.with_tax.formatted}}</span>\r\n            {{#if price.without_tax}}\r\n                {{lang 'products.price_with_tax' tax_label=price.tax_label}}\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if price.without_tax}}\r\n        <div class=\"price-section price-section--withoutTax {{#if price.with_tax}}price-section--minor{{/if}}\"  {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_without_tax}}\r\n                <span data-product-rrp-without-tax class=\"price price--rrp\">{{price.rrp_without_tax.formatted}}</span>\r\n            {{/if}}\r\n            {{#if schema_org}}\r\n                <meta itemprop=\"price\" content=\"{{price.without_tax.value}}\">\r\n                <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\r\n                <meta itemprop=\"availability\" content=\"{{product.availability}}\">\r\n                <meta itemprop=\"itemCondition\" itemtype=\"http://schema.org/OfferItemCondition\" content=\"http://schema.org/{{product.condition}}Condition\">\r\n            {{/if}}\r\n                {{#if theme_settings.price_as_low_as}}\r\n                    <span translate class=\"lowas\">Starting at </span>\r\n                {{/if}}\r\n            <span data-product-price-without-tax class=\"price price--withoutTax\">{{price.without_tax.formatted}}</span>\r\n            {{#if price.with_tax}}\r\n                <abbr title=\"{{lang 'products.excluding_tax'}}\">{{lang 'products.price_without_tax' tax_label=price.tax_label}}</abbr>\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if page_type '===' 'product'}}\r\n        {{#if price.saved}}\r\n            <div class=\"price-section price-section--saving\">\r\n                <span class=\"price\">\r\n                    {{lang 'products.you_save' amount=price.saved.formatted}}\r\n                </span>\r\n            </div>\r\n        {{/if}}\r\n    {{/if}}\r\n{{else}}\r\n    {{#if price.with_tax}}\r\n        <div class=\"price-section price-section--withTax\" {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_with_tax}}\r\n                <span data-product-rrp-with-tax class=\"price price--rrp\">{{price.rrp_with_tax.formatted}}</span>\r\n            {{/if}}\r\n            <span data-product-price-with-tax class=\"price\">{{price.with_tax.formatted}}</span>\r\n            {{#if price.without_tax}}\r\n                {{lang 'products.price_with_tax' tax_label=price.tax_label}}\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if price.without_tax}}\r\n        <div class=\"price-section price-section--withoutTax {{#if price.with_tax}}price-section--minor{{/if}}\"  {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n            {{#if price.rrp_without_tax}}\r\n                <span data-product-rrp-without-tax class=\"price price--rrp\">{{price.rrp_without_tax.formatted}}</span>\r\n            {{/if}}\r\n            {{#if schema_org}}\r\n                <meta itemprop=\"price\" content=\"{{price.without_tax.value}}\">\r\n                <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\r\n            {{/if}}\r\n            <span data-product-price-without-tax class=\"price price--withoutTax\">{{price.without_tax.formatted}}</span>\r\n            {{#if price.with_tax}}\r\n                <abbr title=\"{{lang 'products.excluding_tax'}}\">{{lang 'products.price_without_tax' tax_label=price.tax_label}}</abbr>\r\n            {{/if}}\r\n        </div>\r\n    {{/if}}\r\n    {{#if page_type '===' 'product'}}\r\n        {{#if price.saved}}\r\n            <div class=\"price-section price-section--saving\">\r\n                <span class=\"price\">\r\n                    {{lang 'products.you_save' amount=price.saved.formatted}}\r\n                </span>\r\n            </div>\r\n        {{/if}}\r\n    {{/if}}\r\n    {{price.non_sale_price_without_tax.formatted}}\r\n\r\n{{/or}}\r\n","components/products/product-view":"<div class=\"productView\" id=\"defaulttemplate\">\r\n    {{#each product.reviews.messages}}\r\n        {{#if error}}\r\n            {{> components/common/alert-error error}}\r\n        {{/if}}\r\n        {{#if success}}\r\n            {{> components/common/alert-success success}}\r\n        {{/if}}\r\n    {{/each}}\r\n\r\n    <div class=\"productView-imagesAndDetails\">\r\n      <section class=\"productView-images\" data-image-gallery>\r\n          <div id=\"desk-gallery\">\r\n              <figure class=\"productView-image\"\r\n                      data-image-gallery-main\r\n                      data-zoom-image=\"{{getImage product.main_image 'zoom_size' (cdn theme_settings.default_image_product)}}\"\r\n                      >\r\n\r\n                  <a href=\"{{getImage product.main_image 'zoom_size' (cdn theme_settings.default_image_product)}}\">\r\n                      <img class=\"productView-image--default\"\r\n                           src=\"{{getImage product.main_image 'product_size' (cdn theme_settings.default_image_product)}}\"\r\n                           alt=\"{{#each product.custom_fields}}{{#if name '===' 'Image Alt Text'}}{{value}}{{/if}}{{/each}}\" title=\"{{#each product.custom_fields}}{{#if name '===' 'Image Alt Text'}}{{value}}{{/if}}{{/each}}\" data-main-image>\r\n                  </a>\r\n              </figure>\r\n              <div class=\"menu-wrapperThumbs\">\r\n\r\n                <ul class=\"productView-thumbnails menuThumbs\">\r\n                    {{#each product.images}}\r\n                        <li class=\"productView-thumbnail itemThumb\" data-imgurl=\"{{this.alt}}\">\r\n                            <a\r\n                                class=\"productView-thumbnail-link\"\r\n                                href=\"{{getImage this 'product_size' (cdn ../theme_settings.default_image_product)}}\"\r\n                                data-image-gallery-item\r\n                                data-image-gallery-new-image-url=\"{{getImage this 'product_size' (cdn ../theme_settings.default_image_product)}}\"\r\n                                data-image-gallery-zoom-image-url=\"{{getImage this 'zoom_size' (cdn ../theme_settings.default_image_product)}}\">\r\n                                <img src=\"{{getImage this 'productview_thumb_size' (cdn ../theme_settings.default_image_product)}}\" onerror=\"this.style.display = 'none'; $(this).parent('a').parent('li').hide();\" alt=\"{{#each product.custom_fields}}{{#if name '===' 'Image Alt Text'}}{{value}}{{/if}}{{/each}}\" title=\"{{#each product.custom_fields}}{{#if name '===' 'Image Alt Text'}}{{value}}{{/if}}{{/each}}\">\r\n                            </a>\r\n                        </li>\r\n                    {{/each}}\r\n                </ul>\r\n\r\n                <div class=\"paddles\">\r\n              \t\t<button class=\"left-paddle paddle hidden\">\r\n              \t\t\t<img src=\"{{cdn '/img/arrowLeft.png'}}\">\r\n              \t\t</button>\r\n              \t\t<button class=\"right-paddle paddle hidden\">\r\n              \t\t\t<img src=\"{{cdn '/img/arrowRight.png'}}\">\r\n              \t\t</button>\r\n              \t</div>\r\n\r\n              </div>\r\n\r\n          <div id=\"mainpics\"></div>\r\n\r\n\r\n\r\n          {{> components/common/share}}\r\n\r\n      </section>\r\n\r\n\r\n      <section class=\"productView-details\">\r\n          <div class=\"productView-product\">\r\n              <h1 class=\"productView-title\" {{#if schema}}itemprop=\"name\"{{/if}}>{{product.title}}</h1>\r\n\r\n              <div class=\"productView-rating\"{{#if schema}} itemprop=\"aggregateRating\" itemscope itemtype=\"http://schema.org/AggregateRating\"{{/if}}>\r\n                {{#if product.num_reviews '>' 0}}\r\n                  {{#if schema}}\r\n                      <meta itemprop=\"ratingValue\" content=\"{{product.rating}}\">\r\n                      <meta itemprop=\"ratingCount\" content=\"{{product.num_reviews}}\">\r\n                      <meta itemprop=\"reviewCount\" content=\"{{product.num_reviews}}\">\r\n                  {{/if}}\r\n                  {{> components/products/ratings rating=product.rating}}\r\n                {{/if}}\r\n                {{#if product.num_reviews '>' 0}}\r\n                  <span class=\"productView-reviewLink\">\r\n                    {{lang 'products.reviews.link_to_review' total=product.num_reviews}}\r\n                    <a href=\"{{product.url}}#product-reviews\">Read Reviews</a>\r\n                  </span>\r\n                  <span class=\"productView-reviewLink\">\r\n                      &nbsp;\r\n                  </span>\r\n                  <!--<span class=\"productView-reviewLink\">\r\n                      <a href=\"{{product.url}}{{#if is_ajax}}#write_review{{/if}}\" {{#unless is_ajax }}data-reveal-id=\"modal-review-form\"\r\n                          {{/unless}}>{{lang 'products.reviews.new'}}\r\n                      </a>\r\n                  </span>-->\r\n                {{/if}}\r\n              </div>\r\n\r\n              {{#if product.sku}}\r\n                <dl class=\"productView-info\">\r\n                  <dt class=\"productView-info-name\">{{lang 'products.sku'}}</dt>\r\n                  <dd class=\"productView-info-value\" id=\"productsku\" data-product-sku=\"{{product.sku}}\">{{product.sku}}</dd>\r\n                </dl>\r\n              {{/if}}\r\n              {{product.detail_messages}}\r\n          </div>\r\n\r\n          <div id=\"product-icons\">\r\n              <div class=\"pibox\" id=\"icon-freeship\">\r\n                  <img src=\"{{cdn '/img/truck.png'}}\" alt=\"Free Shipping\" />\r\n                  <span>Free Shipping</span>\r\n              </div>\r\n              <div class=\"pibox\" id=\"icon-hand\"  style=\"display: none;\">\r\n                  <img src=\"{{cdn '/img/btn_handmade.png'}}\" alt=\"Hand Made\" />\r\n                  <span>Hand Made</span>\r\n              </div>\r\n              <div class=\"pibox\" id=\"icon-earth\" style=\"display: none;\">\r\n                  <img src=\"{{cdn '/img/btn_ecofriendly.png'}}\" alt=\"Earth Friendly\" />\r\n                  <span>Earth Friendly</span>\r\n              </div>\r\n              <div class=\"pibox\" id=\"icon-usa\" style=\"display: none;\">\r\n                  <img src=\"{{cdn '/img/btn_flag.png'}}\" alt=\"Made in USA\" />\r\n                  <span>Made in USA</span>\r\n              </div>\r\n          </div>\r\n\r\n          <div id=\"productLinktoDetails\" class=\"productView-info-value\"></div>\r\n\r\n          <div style=\"clear: both;\"></div>\r\n\r\n          <div class=\"productView-options\">\r\n              <form class=\"form\" method=\"post\" action=\"{{product.cart_url}}\" enctype=\"multipart/form-data\"\r\n                    data-cart-item-add>\r\n                  <input type=\"hidden\" name=\"action\" value=\"add\">\r\n                  <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\"/>\r\n                      {{#each product.customizations}}\r\n                          {{{dynamicComponent 'components/products/customizations'}}}\r\n                      {{/each}}\r\n                  <div data-product-option-change style=\"display:none;\">\r\n                      {{#each product.options}}\r\n                          {{{dynamicComponent 'components/products/options'}}}\r\n                      {{/each}}\r\n                  </div>\r\n\r\n                  <div class=\"form-field form-field--stock{{#unless product.stock_level}} u-hiddenVisually{{/unless}}\">\r\n                      <label class=\"form-label form-label--alternate\">\r\n                          {{lang 'products.current_stock'}}\r\n                          <span data-product-stock>{{product.stock_level}}</span>\r\n                      </label>\r\n                  </div>\r\n\r\n                  <div id=\"atcwrap\">\r\n\r\n                      <div class=\"productView-price\">\r\n                          {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                              {{> components/products/price price=product.price schema_org=schema}}\r\n                          {{else}}\r\n                              {{> components/common/login-for-pricing}}\r\n                          {{/or}}\r\n                      </div>\r\n                      {{{region name=\"product_below_price\"}}}\r\n\r\n                      {{#if product.availability}}\r\n                        <div id=\"shiptime\">{{product.availability}}</div>\r\n                      {{/if}}\r\n\r\n                      {{#if product.can_purchase}}\r\n                          {{> components/products/add-to-cart}}\r\n                      {{/if}}\r\n\r\n                      {{#if product.out_of_stock}}\r\n                          {{#if product.out_of_stock_message}}\r\n                              {{> components/common/alert-error product.out_of_stock_message}}\r\n                          {{else}}\r\n                              {{> components/common/alert-error (lang 'products.sold_out')}}\r\n                          {{/if}}\r\n                      {{/if}}\r\n                  </div>\r\n              </form>\r\n              {{#if settings.show_wishlist}}\r\n                {{> components/common/wishlist-dropdown}}\r\n              {{/if}}\r\n          </div>\r\n\r\n\r\n          {{#if product.call_for_price}}\r\n              <p class=\"productView-price\">\r\n                  <span>{{product.call_for_price}}</span>\r\n              </p>\r\n          {{/if}}\r\n\r\n\r\n   <!-- NEW ADDED CONTENT ODD  -->\r\n\r\n\r\n          {{#if product.related_products}}\r\n            <section class=\"boughtTogether\">\r\n              <p class=\"boughtTogetherHeading\">\r\n                <strong>FREQUENTLY BOUGHT TOGETHER</strong>\r\n              </p>\r\n              <div class=\"boughtTogetherBody\">\r\n                <div class=\"boughtTogetherLeft\">\r\n                  <ul>\r\n                    <li>\r\n                      <a href=\"{{url}}\" class=\"card-thumb cardthumb-{{id}} morecolors\">\r\n                        <img class=\"card-image lazyload maint\" data-sizes=\"auto\" src=\"{{getImage product.main_image 'productgallery_size' (cdn theme_settings.default_image_product)}}\" alt=\"{{#each product.custom_fields}}{{#if name '===' 'Image Alt Text'}}{{value}}{{/if}}{{/each}}\" title=\"{{#each product.custom_fields}}{{#if name '===' 'Image Alt Text'}}{{value}}{{/if}}{{/each}}\" title=\"{{#each custom_fields}}{{#if name '===' 'Image Alt Text'}}{{value}}{{/if}}{{/each}}\">\r\n                      </a>\r\n                      <div class=\"card-body\">\r\n                        <div class=\"card-title\">\r\n                          <a href=\"{{url}}\">{{product.title}}</a>\r\n                        </div>\r\n                        <div class=\"productView-price priceReformatted\">\r\n                            {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                                {{> components/products/price price=product.price schema_org=schema}}\r\n                            {{else}}\r\n                                {{> components/common/login-for-pricing}}\r\n                            {{/or}}\r\n                        </div>\r\n                      </div>\r\n                    </li>\r\n                  </ul>\r\n                </div>\r\n\r\n                <div class=\"boughtTogetherCenter\">\r\n                  <p>+</p>\r\n                </div>\r\n\r\n                <div class=\"boughtTogetherRight\">\r\n                  {{> components/products/grid3 products=product.related_products}}\r\n                </div>\r\n              </div>\r\n            </section>\r\n          {{/if}}\r\n\r\n\r\n   <!-- END NEW ADDED CONTENT ODD  -->\r\n\r\n\r\n          {{{snippet 'product_details'}}}\r\n      </section>\r\n    </div>\r\n\r\n  <!-- NEW ADDED CONTENT ODD  -->\r\n\r\n    <section class=\"productView-recentsODD\">\r\n      {{#if customer.recently_viewed_products}}\r\n            <p class=\"headingODD\"><strong>RECENTLY <br>VIEWED</strong></p>\r\n\r\n            <div class=\"menu-wrapperRecents\">\r\n            \t<div class=\"menuRecents\">\r\n                {{#each customer.recently_viewed_products.items}}\r\n                        <a href=\"{{url}}\">\r\n                          <img\r\n                               class=\"itemRecent\"\r\n                               src=\"{{getImage image 'product_size' (cdn ../theme_settings.default_image_product)}}\"\r\n                               alt=\"{{image.alt}}\"\r\n                               title=\"{{image.alt}}\" style=\"border:solid 1px #ffffff; padding:5px;\"/>\r\n                        </a>\r\n                {{/each}}\r\n            \t</div>\r\n\r\n            \t<div class=\"paddles\">\r\n            \t\t<button class=\"up-paddle paddle hidden\">\r\n            \t\t\t<img src=\"{{cdn '/img/arrowUp.png'}}\">\r\n            \t\t</button>\r\n            \t\t<button class=\"down-paddle paddle hidden\">\r\n            \t\t\t<img src=\"{{cdn '/img/arrowDown.png'}}\">\r\n            \t\t</button>\r\n            \t</div>\r\n\r\n            </div>\r\n\r\n      {{/if}}\r\n    </section>\r\n\r\n  <!-- END NEW ADDED CONTENT ODD  -->\r\n\r\n    <div id=\"AnchorDetailandSpecs\"></div>\r\n    <article id=\"productView-description\" class=\"productView-description\"{{#if schema}} itemprop=\"description\"{{/if}}>\r\n        {{#if theme_settings.show_product_details_tabs}}\r\n            {{> components/products/description-tabs}}\r\n        {{else}}\r\n            {{> components/products/description}}\r\n        {{/if}}\r\n    </article>\r\n\r\n    <div id=\"otherdets\">\r\n      {{#each product.custom_fields}}\r\n          <dt class=\"productView-info-name\">{{name}}:</dt>\r\n          <dd class=\"productView-info-value {{name}}\">{{{value}}}</dd>\r\n      {{/each}}\r\n    </div>\r\n\r\n    <div id=\"cornerswatch\" class=\"hideswatch\">\r\n        <div id=\"swatchzoom\"><i class=\"fas fa-search-plus\"></i></div>\r\n        <img id=\"swatchbimg\" src=\"{{getImage product.main_image 'zoom_size' (cdn theme_settings.default_image_product)}}\" onerror=\"this.src = '{{getImage product.main_image 'zoom_size' (cdn theme_settings.default_image_product)}}'\" />\r\n    </div>\r\n</div>\r\n\r\n<div id=\"previewModal\" class=\"modal modal--large\" data-reveal>\r\n    <a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" role=\"button\">\r\n        <span aria-hidden=\"true\">&#215;</span>\r\n    </a>\r\n    <div class=\"modal-content\"></div>\r\n    <div class=\"loadingOverlay\"></div>\r\n</div>\r\n","components/products/ratings":"{{#for 1 5}}\r\n    {{#if ../this.rating '>=' $index}}\r\n        <span class=\"icon icon--ratingFull\">\r\n            <svg>\r\n                <use xlink:href=\"#icon-star\" />\r\n            </svg>\r\n        </span>\r\n    {{else}}\r\n        <span class=\"icon icon--ratingEmpty\">\r\n            <svg>\r\n                <use xlink:href=\"#icon-star\" />\r\n            </svg>\r\n        </span>\r\n    {{/if}}\r\n{{/for}}\r\n\r\n{{{snippet 'product_rating'}}}\r\n"}
