{"components/products/options/set-select":"<div class=\"form-field {{this.display_name}}\" data-product-attribute=\"set-select\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\r\n         <span class=\"mobhide\">PLEASE SELECT</span> {{ this.display_name }}\r\n    </label>\r\n\r\n    <select class=\"form-select form-select--small\" name=\"attribute[{{this.id}}]\" id=\"attribute_select_{{id}}\" {{#if required}}required{{/if}}>       \r\n        {{#each this.values}}\r\n            <option data-product-attribute-value=\"{{id}}\" value=\"{{id}}\" {{#if selected}}selected{{/if}}> {{#if label '===' 'Serged Cotton'}} Serged Cotton + $8.00 {{else}}{{label}}{{/if}}</option>\r\n        {{/each}}\r\n    </select>\r\n</div>\r\n"}
