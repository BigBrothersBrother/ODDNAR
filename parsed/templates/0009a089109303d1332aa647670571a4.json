{"components/category/sort-box":"<fieldset class=\"form-fieldset actionBar-section\">\r\n    <div class=\"form-field\">\r\n        <label class=\"form-label\" for=\"sort\">{{lang 'common.sorter.sort_by'}}</label>\r\n        <select class=\"form-select form-select--small\" name=\"sort\" id=\"sort\">\r\n            <option data-sort=\"bestselling\" value=\"bestselling\" {{#if sort '==' 'bestselling'}}selected{{/if}}>{{lang 'common.sorter.top_sellers'}}</option>\r\n            <option data-sort=\"featured\" value=\"featured\" {{#if sort '==' 'featured'}}selected{{/if}}>{{lang 'common.sorter.featured'}}</option>     \r\n            <option data-sort=\"alphaasc\" value=\"alphaasc\" {{#if sort '==' 'alphaasc'}}selected{{/if}}>{{lang 'common.sorter.alpha_asc'}}</option>\r\n            <option data-sort=\"alphadesc\" value=\"alphadesc\" {{#if sort '==' 'alphadesc'}}selected{{/if}}>{{lang 'common.sorter.alpha_desc'}}</option>\r\n            <option data-sort=\"avgcustomerreview\" value=\"avgcustomerreview\" {{#if sort '==' 'avgcustomerreview'}}selected{{/if}}>{{lang 'common.sorter.reviews'}}</option>\r\n            <option data-sort=\"priceasc\" value=\"priceasc\" {{#if sort '==' 'priceasc'}}selected{{/if}}>{{lang 'common.sorter.price_asc'}}</option>\r\n            <option data-sort=\"pricedesc\" value=\"pricedesc\" {{#if sort '==' 'pricedesc'}}selected{{/if}}>{{lang 'common.sorter.price_desc'}}</option>\r\n            {{#if result_count}}\r\n                <option value=\"relevance\" {{#if sort '==' 'relevance'}}selected{{/if}}>{{lang 'common.sorter.relevance'}}</option>\r\n            {{/if}}\r\n        </select>\r\n    </div>\r\n</fieldset>\r\n","components/products/filter":"<form class=\"actionBar\" method=\"get\" data-sort-by>\r\n    {{> components/category/sort-box sort=sort}}\r\n\r\n    <label for=\"sort-box\">Sort:</label>\r\n    <ul id=\"sort-box\" aria-hidden=\"true\">\r\n        <li data-sort=\"bestselling\">Best Selling</li>\r\n        <li data-sort=\"featured\" class=\"active-sort\">Featured Items</li>\r\n        <!--<li data-sort=\"newest\">New Arrivals</li>-->\r\n        <!--<li data-sort=\"avgcustomerreview\">{{lang 'common.sorter.reviews'}}</li>-->\r\n        <li data-sort=\"priceasc\">$ - Low</li>\r\n        <li data-sort=\"pricedesc\">$$$ - High</li>\r\n    </ul>\r\n</form>\r\n"}
