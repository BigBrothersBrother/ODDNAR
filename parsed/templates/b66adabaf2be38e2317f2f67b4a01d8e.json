{"components/cart/preview":"<div style=\"border-bottom:solid 1px #ebebeb; padding-top:15px; padding-bottom:15px;\">\r\n    <h2 class=\"title-heading\">The following items have been added to your cart</h2></div>\r\n<div class=\"table-items\">\r\n    <table>\r\n        <tr class=\"thead\">\r\n            <th style=\"text-align:left;\">Product Details</th>\r\n            <th>...</th>\r\n            <th>QTY</th>\r\n        </tr>\r\n        {{#with cart.added_item}}\r\n        <tr class=\"trow\" style=\"border-bottom: solid 1px #000;\">\r\n            <td style=\"text-align:left;\">\r\n                <img class=\"productView-image--cart\"\r\n                     src=\"{{getImage image 'product_size' (cdn ../theme_settings.default_image_product)}}\"\r\n                     alt=\"{{image.alt}}\"\r\n                     title=\"{{image.alt}}\" width=\"140\" style=\"border:solid 1px #ebebeb; padding:5px;\"/>\r\n            </td>\r\n            <td>\r\n                <div style=\"padding:5px;\"><strong>{{name}}</strong></div>\r\n                <div>\r\n                    <table class=\"table-specs\">\r\n                        <tr>\r\n                            <td class=\"label\"><strong>Brand:</strong></td>\r\n                            <td>{{brand.name}}</td>\r\n                        </tr>\r\n\r\n                        {{#if configurable_fields}}\r\n                        {{#each configurable_fields}}\r\n                        <tr>\r\n                            <td class=\"label\">\r\n                                <strong>{{name}}:</strong>\r\n                            </td>\r\n                            <td>\r\n                                {{#if is_file}}\r\n                                <a href=\"/viewfile.php?prodfield={{../id}}&cartitem={{../../id}}\">{{{value}}}</a>\r\n                                {{else}}\r\n                                {{{value}}}\r\n                                {{/if}}\r\n                            </td>\r\n                        </tr>\r\n                        {{/each}}\r\n                        {{/if}}\r\n\r\n                        {{#if options}}\r\n                        {{#each options}}\r\n                        <tr>\r\n                            <td class=\"label\">\r\n                                <strong>{{name}}:</strong>\r\n                            </td>\r\n                            <td>\r\n                                {{#if is_file}}\r\n                                <a href=\"/viewfile.php?attributeId={{../id}}&cartitem={{../../id}}\">{{{value}}}</a>\r\n                                {{else}}\r\n                                {{{value}}}\r\n                                {{/if}}\r\n                            </td>\r\n                        </tr>\r\n                        {{/each}}\r\n                        {{/if}}\r\n\r\n                        <!--<tr>-->\r\n                        <!--<td class=\"label\"><strong>Price:</strong></td>-->\r\n                        <!--<td>-->\r\n                        <!--    {{#or ../customer (if ../theme_settings.restrict_to_login '!==' true)}}-->\r\n                        <!--    {{quantity}} &times; {{price.formatted}}-->\r\n                        <!--    {{else}}-->\r\n                        <!--    {{> components/common/login-for-pricing}}-->\r\n                        <!--    {{/or}}-->\r\n                        <!--</td>-->\r\n                        <!--</tr>-->\r\n                    </table>\r\n                </div>\r\n            </td>\r\n            <td><strong>{{quantity}}</strong></td>\r\n        </tr>\r\n        {{/with}}\r\n        <tr style=\"border-top: solid 2px #ebebeb;\">\r\n            <td colspan=\"3\" style=\"text-align:right;\">\r\n                <a href=\"#\" class=\"button continue-shopping\" role=\"button\" data-reveal-close style=\"background-color:#ffffff; color:#408300; font-weight:bold;\">Continue shopping</a>\r\n                {{#if cart.show_primary_checkout_button}}\r\n                <a href=\"/cart.php\" class=\"button button--primary\">Checkout</a>\r\n                {{/if}}\r\n            </td>\r\n        </tr>\r\n    </table>\r\n</div>\r\n","components/common/login-for-pricing":"<p translate>Log in for pricing</p>\r\n"}
